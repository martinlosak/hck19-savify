{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel",
    [
      "module-resolver",
      {
        "extensions": [
          ".js",
          ".ts"
        ],
        "root": [
          "."
        ]
      }
    ]
  ]
}
